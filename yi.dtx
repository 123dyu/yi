%   \item 上下爻间距
% \end{arguments}
%    \begin{macrocode}
\cs_new_protected:Npn \@@_yao_stack:Nn #1#2
  {
    \coffin_clear:N  \l_@@_tmp_coffin
    \vcoffin_set:Nnn \l_@@_tmp_coffin 
      { \l_@@_yao_width_dim } 
      { \skip_vertical:n { - #2 } }
