      { \l_@@_yao_yingap_dim } 
      { \l_@@_yao_width_dim  } 
      { \l_@@_yao_height_dim }
    \@@_make_yangyao_coffin:nn 
      { \l_@@_yao_width_dim  } 
      { \l_@@_yao_height_dim }
    \seq_map_inline:Nn #1
      {
        \int_case:nn { ##1 }
          {
            {0}
            {
              \coffin_join:NnnNnnnn 
                \l_@@_tmp_coffin    {l} {t}
                \l_@@_yinyao_coffin {l} {b}
                { 0em } {#2}
            }
            {1}
            {
